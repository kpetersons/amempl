tr(class='#{account_selected} #{account_balance_class}')
	td.span2
		a.icon-edit(href='/accounts/#{account._id}/edit')
		!&nbsp;
		a.details(href='#')=account_name
	td=account_description
	td.span1
		- if (account_default)
			.icon-star
		- else
			a.default.icon-star-empty(href='#')
	td.span2
		.text-right=account_balance
	td.span1
		.pull-right
			a.icon-trash(href='/accounts/#{account._id}/delete')
