include 'transactions'
.modal.transactions.new
	.modal-header
		h4 New
	-if(transaction.messages)
		ul.unstyled.alert.alert-error
			foreach transaction.messages as field
				li=field.message
	.modal-body
		include 'form' exposing transaction
	.modal-footer
		.btn-group.pull-right
			button.save.btn.btn-primary Save
			button.cancel.btn Cancel
	
